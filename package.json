{
  "name": "trutrip-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:clean": "vitest run src/components/ui/__tests__/Button.test.tsx src/components/ui/__tests__/Typography.test.tsx src/components/shared/__tests__/Navbar.test.tsx src/components/shared/__tests__/Pagination.test.tsx src/components/trip/__tests__/TripPriorityBadge.test.tsx src/components/trip/__tests__/TripStatusBadge.test.tsx",
    "test:individual": "echo 'Run individual tests like: pnpm vitest run src/components/ui/__tests__/Button.test.tsx'",
    "test:help": "echo 'Available test commands:\n  test:clean - Run 59 clean tests (no mocking, 100% pass rate)\n  test:individual - Run single test files\n  test:ui - Start test UI\n  test - Run all tests (may have EMFILE issues)'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.121.27",
    "@tanstack/react-router-devtools": "^1.121.27",
    "date-fns": "^4.1.0",
    "i18next": "^25.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tanstack/eslint-plugin-query": "^4.39.1",
    "@tanstack/router-plugin": "^1.121.27",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  }
